<script>
myID = document.getElementById("freeze");

var myScrollFunc = function () {
    var y = window.scrollY;
    if (y >= 4050) {
        myID.style.display = 'block';
    } else {
        myID.style.display = 'none';
    }
};

window.addEventListener("scroll", myScrollFunc);
</script>
