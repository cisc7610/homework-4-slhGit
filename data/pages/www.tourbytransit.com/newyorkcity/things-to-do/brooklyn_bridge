<script>
var directionDisplay,map,directionsService=new google.maps.DirectionsService,geocoder=new google.maps.Geocoder,styleArray=[{featureType:"all",stylers:[{saturation:-20},{gamma:.6}]}];

function initialize() {
  
	var latlng = new google.maps.LatLng(40.706074,-73.996943);
	var rendererOptions = {	draggable: true	};
	directionsDisplay = new google.maps.DirectionsRenderer(rendererOptions);
	var myOptions = {
		zoom: 14,
		center: latlng,
		mapTypeId: google.maps.MapTypeId.ROADMAP,
		mapTypeControl: true,
        scaleControl: true,
	};

	initialize.map = map = new google.maps.Map(document.getElementById("map-canvas"),myOptions);
	directionsDisplay.setMap(map);
	
	var pinColor = "DF6F03";
    var pinImage = new google.maps.MarkerImage("//chart.apis.google.com/chart?chst=d_map_pin_letter&chld=%E2%80%A2|" + pinColor,
        new google.maps.Size(21, 34),
        new google.maps.Point(0,0),
        new google.maps.Point(10, 34));

  var marker = new google.maps.Marker({
    position: latlng,
    map: map,
    icon: pinImage
  });
	
  var transitLayer = new google.maps.TransitLayer();
  transitLayer.setMap(map);
  			           
  var panoramaOptions = {
    position: latlng,
    scrollwheel: false,
    pov: {
      heading: 320,
      pitch: 10,
    zoom:1.2,
    }
  };
  var panorama = new google.maps.StreetViewPanorama(document.getElementById('pano'), panoramaOptions);
  map.setStreetView(panorama);

		var attractionsLayer = new google.maps.KmlLayer({
    			url: 'https://cdn.tourbytransit.com/kml/NYC-Brooklyn-Bridge-Walk.kml',
    			preserveViewport: true,
            suppressInfoWindows: false,
  		});
  			attractionsLayer.setMap(map);

	directionsDisplay.setPanel(document.getElementById("directionsPanel"));
	
  return map;
}

function calcRoute(e,o){var t=$('input[name="travelMode"]:checked').val(),i=$("#routeStart").val(),n=$("#routeEnd").val(),s={origin:i,destination:n,unitSystem:google.maps.UnitSystem.METRIC,travelMode:google.maps.DirectionsTravelMode[t]};directionsService.route(s,function(t,i){i==google.maps.DirectionsStatus.OK?($("#directionsPanel").empty(),directionsDisplay.setDirections(t),(e||function(){})()):("ZERO_RESULTS"==i?alert("No route could be found for the selected mode. Please try again with a different mode."):"UNKNOWN_ERROR"==i?console.log("A directions request could not be processed due to a server error. The request may succeed if you try again."):"REQUEST_DENIED"==i?console.log("This webpage is not allowed to use the directions service."):"OVER_QUERY_LIMIT"==i?(console.log("This page has exceeded the request limit in too short a period of time. Please wait a few seconds and try again."),console.log("Missing or invalid starting point. Please re-enter your location and try again.")):"INVALID_REQUEST"==i?console.log("The DirectionsRequest provided was invalid."):console.log("There was an unknown error in your request. Requeststatus: \n\n"+i),(o||function(){})())})}
</script>
<script>
$(".loadMap").on("click",function(){initialize(),$(".loadMap").hide()});var locateMe=$("#locateMe"),foundMe=$("#foundMe"),loadedDirections=function(){},hasFoundYou=!1,changedLocation=!1;locateMe.on("click",function(){var o=initialize();changedLocation=!1,o.addListener("tilesloaded",function(){navigator.geolocation?hasFoundYou?loadedDirections():(hasFoundYou=!0,navigator.geolocation.getCurrentPosition(function(e){var n=new google.maps.LatLng(e.coords.latitude,e.coords.longitude);geocoder.geocode({latLng:n},function(e,a){if(a==google.maps.GeocoderStatus.OK){if(e[0]){marker=new google.maps.Marker({position:n,map:o});var t=e[0].address_components[1].long_name+" "+e[0].address_components[0].long_name+", "+e[0].address_components[3].long_name;$("#routeStart").val(t),calcRoute(function(){locateMe.hide(),loadedDirections()}),$(".loadMap").hide()}}else alert("Geocoder failed due to: "+a)})},function(){})):console.log("Geolocation not supported or not enabled.")})}),$("#routeStart").change(function(){changedLocation=!0}),$(".scrollDirections").click(function(){hasFoundYou&&!changedLocation?loadedDirections():locateMe.click()}),$(".scrollDirections").click(function(){$("html, body").animate({scrollTop:$("#directionsPanel").offset().top -300})});
</script>

